{
  "name": "crud-react-node",
  "version": "1.0.0",
  "description": "CRUD app with Node as backend and React as frontend",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node src/server-es5.js",
    "start:dev": "cross-env NODE_ENV=development && nodemon src/server.js",
    "build": "cross-env NODE_ENV=production webpack -p --profile --display-modules --optimize-minimize",
    "build:dev": "webpack -d",
    "build:dev:watch": "webpack -d --watch"
  },
  "author": "Daniel Cedergren",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.13.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "jquery": "^3.3.1",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "popper.js": "^1.14.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-froala-wysiwyg": "^2.7.6",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "file-loader": "^1.1.11",
    "nodemon": "^1.17.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-node-externals": "^1.6.0"
  }
}
